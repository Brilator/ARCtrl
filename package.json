{
  "name": "arcapi",
  "version": "1.0.0",
  "description": "Top level ARC DataModel and API function descriptions.",
  "type": "module",
  "scripts": {
    "fable-js": "dotnet fable ./src/ARC -o ./js",
    "fable-ts": "dotnet fable ./src/ARC -o ./ts --lang ts",
    "pretestJson": "dotnet fable tests/ISA/ISA.Json.Tests -o tests/ISA/ISA.Json.JsNativeTests/fable",
    "testJson": "mocha tests/ISA/ISA.Json.JsNativeTests/fable",
    "posttestJson": "mocha tests/ISA/ISA.Json.JsNativeTests",
    "testJson:live": "mocha --watch --parallel tests/ISA/ISA.Json.JsNativeTests/fable",
    "testJsonnative:live": "mocha --watch --parallel tests/ISA/ISA.Json.JsNativeTests",
    "pretest": "dotnet fable tests/ISA/ISA.Tests -o tests/ISA/ISA.JsNativeTests/fable",
    "test": "mocha tests/ISA/ISA.JsNativeTests/fable",
    "posttest": "mocha tests/ISA/ISA.JsNativeTests",
    "test:live": "mocha --watch --parallel tests/ISA/ISA.JsNativeTests/fable",
    "testnative:live": "mocha --watch --parallel tests/ISA/ISA.JsNativeTests"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nfdi4plants/arcAPI.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/nfdi4plants/arcAPI/issues"
  },
  "homepage": "https://github.com/nfdi4plants/arcAPI#readme",
  "dependencies": {
    "fable-library": "^1.1.1"
  },
  "devDependencies": {
    "mocha": "^10.2.0"
  }
}
